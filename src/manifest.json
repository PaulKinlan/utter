{
  "manifest_version": 3,
  "name": "Utter",
  "version": "1.0.0",
  "description": "Chrome extension",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    }
  },
  "permissions": [],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
